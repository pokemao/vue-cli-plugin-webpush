{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "moduleSuffixes": [""],
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": true,
  }
}
